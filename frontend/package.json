{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve pc-client",
    "start:pc": "ng serve pc-client --port 4200",
    "start:phone": "ng serve phone-client --port 4201",
    "build": "ng build shared && ng build pc-client && ng build phone-client",
    "build:shared": "ng build shared",
    "build:pc": "ng build pc-client",
    "build:phone": "ng build phone-client",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@9.2.0",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "@types/qrcode": "^1.5.6",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
